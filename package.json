{
  "name": "less-preview",
  "version": "1.0.0",
  "description": "Less Preview",
  "author": "Matthew Dean",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "vue": "^3.2.25",
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "vue3-ace-editor": "^2.2.2"
  }
}
