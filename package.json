{
  "name": "less-preview",
  "version": "1.0.0",
  "description": "Less Preview",
  "author": "Matthew Dean",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.0.0",
    "@codemirror/theme-one-dark": "^6.0.0",
    "axios": "^0.26.1",
    "codemirror": "^6.0.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/less": "^3.0.3",
    "@types/node": "^16.11.12",
    "@vitejs/plugin-vue": "^2.3.3",
    "less": "^3.7.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.17",
    "vue-tsc": "^0.34.7"
  }
}
